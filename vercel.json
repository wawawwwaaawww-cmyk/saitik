{
  "version": 2,
  "builds": [
    {
      "src": "**/*.{html,css,js,png,jpg,jpeg,svg,gif,ico,webp,woff,woff2,ttf,eot}",
      "use": "@vercel/static"
    }
  ],
  "cleanUrls": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/contacts",
      "destination": "/contacts.html"
    },
    {
      "source": "/vyvod-iz-zapoya",
      "destination": "/vyvod-iz-zapoya.html"
    },
    {
      "source": "/snyatie-lomki",
      "destination": "/snyatie-lomki.html"
    },
    {
      "source": "/kodirovanie",
      "destination": "/kodirovanie.html"
    },
    {
      "source": "/snyatie-pohmelya",
      "destination": "/snyatie-pohmelya.html"
    },
    {
      "source": "/kapelnitsa-ot-pohmelya",
      "destination": "/kapelnitsa-ot-pohmelya.html"
    }
  ]
}
